{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./lib/.tsbuildinfo",
    "target": "es2018",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {},
    "typeRoots": [
      "node_modules/@types"
    ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./lib",
    "skipLibCheck": true
  },
  "exclude": [
    "**.js",
    "**.d.ts",
    "lib"
  ]
}
